
@{
    Layout = null;
}

@model IEnumerable<GeyikLoung.Entities.AltKategori>

<h2>Alt Kategoriler</h2>
<ul>
    @foreach (var subCategory in Model)
    {
        <li>
            <a href="@Url.Action("AltKategoriDetails", "Default", new { altKategoriId = subCategory.Id })">
                @subCategory.Name
            </a>
        </li>
    }
</ul>
